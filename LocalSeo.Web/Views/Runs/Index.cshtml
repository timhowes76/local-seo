@model IReadOnlyList<SearchRun>
<h2>Last 20 Runs</h2>
<table>
<thead><tr><th>ID</th><th>Keyword</th><th>Location</th><th>Ran</th><th>Details</th></tr></thead>
<tbody>
@foreach (var r in Model)
{
    <tr>
        <td>@r.SearchRunId</td><td>@r.SeedKeyword</td><td>@r.LocationName</td><td>@r.RanAtUtc</td><td><a href="/runs/@r.SearchRunId">View</a></td>
    </tr>
}
</tbody>
</table>
